{
  "name": "@gorebel/css-class-generator",
  "version": "1.0.1",
  "description": "Generates a sequential, valid CSS class, generating the next smallest class names possible",
  "main": "src/index.js",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/prompt": "^7.2.1",
    "ava": "^0.25.0",
    "commitizen": "^3.0.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "standard-version": "^4.4.0"
  },
  "scripts": {
    "test": "ava -T 60s",
    "cz": "git-cz",
    "release": "standard-version"
  },
  "keywords": [
    "css",
    "class",
    "classname",
    "name",
    "generator",
    "minify",
    "unique",
    "id"
  ],
  "author": "Jacob Parker",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rebelmail/css-class-generator"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
